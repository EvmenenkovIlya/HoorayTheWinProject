<Window x:Class="HoorayTheWinProject_.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:HoorayTheWinProject_"
        mc:Ignorable="d"
        Title="MainWindow" Height="578" Width="894">
    <Grid>
        <TabControl>
            <TabItem Header="Users and Tests" Margin="-2,-2,-35,0">
                <Grid Background="#FFE5E5E5" Height="538" Margin="0,0,-2,0">
                    <Button Content="Update UserList" HorizontalAlignment="Left" Margin="452,354,0,0" VerticalAlignment="Top" Width="106"/>
                    <Button Content="Start / Finish Test" HorizontalAlignment="Left" Margin="473,256,0,0" VerticalAlignment="Top" Height="44" Width="106"/>
                    <ListBox x:Name="ListBoxGroupsName" d:ItemsSource="{d:SampleData ItemCount=5}" Margin="20,26,549,435">
                        <ListBoxItem Content="Test0"/>
                        <ListBoxItem Content="Test1"/>
                        <ListBoxItem Content="Test2"/>
                    </ListBox>
                    <ListBox d:ItemsSource="{d:SampleData ItemCount=5}" Margin="31,238,549,72"/>
                    <Label Content="List of Groups:" HorizontalAlignment="Left" Margin="20,0,0,0" VerticalAlignment="Top" Width="162"/>
                    <TextBox x:Name="TextBoxNewGroupName" HorizontalAlignment="Left" Margin="20,119,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
                    <Button x:Name="ButtonChangeGroupName" Content="Change Group Name" HorizontalAlignment="Left" Margin="163,119,0,0" VerticalAlignment="Top" Width="132"/>
                    <TextBox x:Name="TextBoxNewUserName" HorizontalAlignment="Left" Margin="31,476,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
                    <Button x:Name="ButtonChangeUserName" Content="Change User Name" HorizontalAlignment="Left" Margin="184,476,0,0" VerticalAlignment="Top" Width="137"/>
                    <Label Content="List of Users:" HorizontalAlignment="Left" Margin="32,207,0,0" VerticalAlignment="Top" Width="92"/>
                    <ComboBox HorizontalAlignment="Left" Margin="32,499,0,0" VerticalAlignment="Top" Width="120"/>
                    <Button x:Name="ButtonAddToGroup" Content="Add To Group" HorizontalAlignment="Left" Margin="184,503,0,0" VerticalAlignment="Top" Width="137"/>
                </Grid>
            </TabItem>
            <TabItem Header="Question" Height="20" Margin="29,0,-29,0" VerticalAlignment="Center">
                <Grid Background="#FFE5E5E5">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="89*"/>
                        <ColumnDefinition Width="705*"/>
                    </Grid.ColumnDefinitions>
                    <ComboBox x:Name="ComboBoxTypeOfQuestion" HorizontalAlignment="Left" Margin="23,10,0,0" VerticalAlignment="Top" Width="169" Grid.ColumnSpan="2"/>
                    <Button x:Name="ButtonCreateAQuestion" Content="Create a question" HorizontalAlignment="Left" Margin="88,52,0,0" VerticalAlignment="Top" Width="118" Grid.ColumnSpan="2"/>
                    <TextBox x:Name="TextBoxTypeOfQuestion" HorizontalAlignment="Left" Margin="23,91,0,0" TextWrapping="Wrap" Text="Text of a question" VerticalAlignment="Top" Width="248" Height="100" Grid.ColumnSpan="2"/>
                    <RadioButton Content="RadioButton" HorizontalAlignment="Left" Margin="23,228,0,0" VerticalAlignment="Top" Grid.ColumnSpan="2"/>
                    <RadioButton Content="RadioButton" HorizontalAlignment="Left" Margin="23,257,0,0" VerticalAlignment="Top" Grid.ColumnSpan="2"/>
                    <RadioButton Content="RadioButton" HorizontalAlignment="Left" Margin="23,318,0,0" VerticalAlignment="Top" Grid.ColumnSpan="2"/>
                    <RadioButton Content="RadioButton" HorizontalAlignment="Left" Margin="23,287,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.511,0.493" Grid.ColumnSpan="2"/>
                    <TextBlock HorizontalAlignment="Left" Margin="19,202,0,0" TextWrapping="Wrap" Text="Variants " VerticalAlignment="Top" Width="70" Grid.Column="1"/>
                    <CheckBox Content="CheckBox" HorizontalAlignment="Left" Margin="176,230,0,0" VerticalAlignment="Top" Grid.Column="1"/>
                    <CheckBox Content="CheckBox" HorizontalAlignment="Left" Margin="176,289,0,0" VerticalAlignment="Top" Grid.Column="1"/>
                    <CheckBox Content="CheckBox" HorizontalAlignment="Left" Margin="176,259,0,0" VerticalAlignment="Top" Grid.Column="1"/>
                    <CheckBox Content="CheckBox" HorizontalAlignment="Left" Margin="176,320,0,0" VerticalAlignment="Top" Grid.Column="1"/>
                    <TextBox HorizontalAlignment="Left" Margin="35,228,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120" Grid.Column="1"/>
                    <TextBox HorizontalAlignment="Left" Margin="35,257,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120" Grid.Column="1"/>
                    <TextBox HorizontalAlignment="Left" Margin="35,286,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120" Grid.Column="1"/>
                    <TextBox HorizontalAlignment="Left" Margin="35,314,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120" Grid.Column="1"/>
                </Grid>
            </TabItem>
        </TabControl>
    </Grid>
</Window>
